// Place your key bindings in this file to overwrite the defaults
[
  { "key": "cmd+0", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus" },
  { "key": "cmd+0", "command": "workbench.action.terminal.focus", "when": "!terminalFocus" },
  {"key": "alt+w", "command": "workbench.action.switchWindow"}
  {"key": "alt+right", "command": "workbench.action.increaseViewSize"}
  {"key": "alt+left", "command": "workbench.action.decreaseViewSize"}
  {"key": "alt+down", "command": "workbench.action.terminal.focusNext"}
  {"key": "alt+up", "command": "workbench.action.terminal.focusPrevious"}
  {"key": "alt+t", "command": "workbench.action.terminal.new"}

]